{
    "target_dataset":{
        "name": "qnli",
        "dev_rate": 0.1
    },
    "poison_dataset":{
        "name": "qnli",
        "dev_rate": 0.1
    },
    "victim":{
        "type": "plm",
        "model": "bert",
        "path": "bert-base-uncased",
	"freeze_features": false,
        "num_classes": 2,
        "device": "gpu",
        "max_len": 512,
	"unfreeze_n_top": 1,
	"teacher_name": "textattack/bert-base-uncased-QNLI",
	"kd_temperature": 1,
	"kd_alpha": 0,
	"use_adapter": true
    },
    "attacker":{
        "name": "base",
        "metrics": ["accuracy"],
        "train":{
            "name": "base",
            "lr": 3e-5,
            "weight_decay": 0.01,
            "epochs": 5,
            "batch_size": 8,
            "warm_up_epochs": 1,
            "ckpt": "best",
            "save_path": "./models"
        },
        "poisoner":{
            "name": "stylebkd",
            "poison_rate": 0.01,
            "target_label": 0,
            "label_consistency": false,
            "label_dirty": false,
            "load": false,
            "template_id": 0
        }
    },
    "clean-tune": false


}
